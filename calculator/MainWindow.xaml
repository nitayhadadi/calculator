<Window x:Class="calculator.MainWindow"
                 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Game Page" Height="430" Width="712">
    <Grid>
        <StackPanel>
            <Label Content="What is your name?" />
            <TextBox x:Name="NameTextBox" />
            <Label Content="Age" />
            <TextBox x:Name="AgeTextBox" 
                     />
            <Button Content="Submit" Click="SubmitButton_Click" Height="18" />



            <Image Height="356" Width="634" RenderTransformOrigin="-1.862,0.531">
                <Image.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform AngleX="1.399"/>
                        <RotateTransform/>
                        <TranslateTransform X="-4.079"/>
                    </TransformGroup>
                </Image.RenderTransform>
            </Image>
        </StackPanel>
    </Grid>
</Window>

